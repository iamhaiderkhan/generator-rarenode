{
    "env": {
        "browser": true,
        "es6": true,
        "mocha": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "rules": {
        "indent": [
            2,
            4
        ],
        "linebreak-style": 0,
        "no-console": 0,
        "no-unused-vars": 0,
        "quotes": [
            2,
            "single"
        ],
        "require-atomic-updates": 0,
        "semi": [
            2,
            "always"
        ]
    }
}